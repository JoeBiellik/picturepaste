{
  "name": "picturepaste",
  "port": 3000,
  "db": "mongodb://localhost/paste",
  "cacheAge": 86400,
  "sizeLimit": "50mb",
  "prettyHtml": true,
  "expiresDefault": 604800,
  "expires": {
    "600": "10 minutes",
    "3600": "1 hour",
    "86400": "1 day",
    "604800": "1 week",
    "2592000": "1 month",
    "31536000 ": "1 year"
  },
  "formats": {
    "image/jpeg": ["jpg", "jpeg", "jpe", "jfif"],
    "image/png": ["png", "apng"],
    "image/gif": "gif",
    "image/bmp": ["bmp", "dib"],
    "image/tiff": ["tiff", "tif"],
    "image/webp": "webp",
    "image/svg+xml": "svg",
    "image/x-icon": "ico"
  }
}
